		"hide_md": {
			"type": "boolean",
			"default": false
		},
		"hide_lg": {
			"type": "boolean",
			"default": false
		},
		"hide_xl": {
			"type": "boolean",
			"default": false
		},
		"hide_xxl": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"anchor": true,
		"align": false,
		"html": false
