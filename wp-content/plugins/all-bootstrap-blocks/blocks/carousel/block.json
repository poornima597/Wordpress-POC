{
	"apiVersion": 2,
	"name": "areoi/carousel",
	"title": "Carousel",
	"parent": null,
	"category": "areoi-components",
	"description": "A slideshow component for cycling through elements—images or slides of text—like a carousel.",
	"textdomain": "default",
	"keywords": [ "areoi", "bootstrap", "layout" ],
	"example": {
		"attributes": {
			"preview": true
		}
	},
	"attributes": {
		"preview": {
			"type": "boolean",
			"default": false
		},
		"anchor": {
			"type": "string",
			"default": false
		},
		"block_id": {
			"type": "string",
			"default": null
		},
		"controls": {
			"type": "boolean",
			"default": true
		},
		"indicators": {
			"type": "boolean",
			"default": true
		},
		"touch": {
			"type": "boolean",
			"default": true
		},
		"auto_scroll": {
			"type": "boolean",
			"default": true
		},
		"interval": {
			"type": "string",
			"default": "4000"
		},
		"transition": {
			"type": "string",
			"default": null
		},
		"style": {
			"type": "string",
			"default": null
		},

		"hide_xs": {
			"type": "boolean",
			"default": false
		},
		"hide_sm": {
			"type": "boolean",
			"default": false
		},
		"hide_md": {
			"type": "boolean",
			"default": false
		},
		"hide_lg": {
			"type": "boolean",
			"default": false
		},
		"hide_xl": {
			"type": "boolean",
			"default": false
		},
		"hide_xxl": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"anchor": true,
		"align": false,
		"html": false
	},
	"editorScript": "areoi-blocks",
    "editorStyle": "file:./index.css",
    "style": "file:../../build/style.css"
}
